prototype(GSL.DuttweilerDe.API:EventsList) < prototype(Neos.Fusion:Http.Message) {
	httpResponseHead {
		headers.Content-Type = 'application/json;charset=utf-8'
	}
	content = GSL.DuttweilerDe.Content:EventPromoter {
		templatePath = 'resource://GSL.DuttweilerDe/Private/Templates/API/GenericList.html'
		listCollection = ${q(eventsNode).children().children('[instanceof GSL.DuttweilerDe.Content:Event]').get()}
		listItem = GSL.DuttweilerDe.Content:Event {
			templatePath = 'resource://GSL.DuttweilerDe/Private/Templates/API/EventsItem.html'
		}
	}

	@cache {
		mode = 'cached'

		entryTags {
			1 = ${'NodeType_GSL.DuttweilerDe.Content:Event'}
		}
	}
}
